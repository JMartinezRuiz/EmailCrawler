SELECT
  gifts.users_ver.user_id,
  gifts.users_ver.enable  AS users_ver_enable,
  gifts.users_ver.deleted AS users_ver_deleted,
  gifts.users.enable      AS users_enable,
  gifts.users.deleted     AS users_deleted
FROM gifts.users_ver
LEFT JOIN gifts.users
  ON gifts.users.user_id = gifts.users_ver.user_id
WHERE gifts.users_ver.deleted = 'Y'
  AND (gifts.users.user_id IS NULL OR gifts.users.deleted <> 'Y');
